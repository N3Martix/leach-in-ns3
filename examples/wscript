## -*- Mode: python; py-indent-offset: 4; indent-tabs-mode: nil; coding: utf-8; -*-

def build(bld):
     obj = bld.create_ns3_program('leach-proposal', ['wifi', 'internet', 'leach', 'applications', 'netanim'])
     obj.source = 'leach-proposal.cc'

     obj = bld.create_ns3_program('lr-wpan-real', ['wifi', 'internet', 'applications', 'lr-wpan'])
     obj.source = 'lr-wpan-real.cc'

     obj = bld.create_ns3_program('lr-wpan-test', ['lr-wpan'])
     obj.source = 'lr-wpan-test.cc'
